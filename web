  el java script

var person ={
        name:"seif",
        major:"Data & AI ",
        hello:function(){
                console.log("welcome "+this.name)

        }
}
person.hello()
console.log(person.major)
person.name="sara"
console.log(person)
person.age=28
console.log(person)
delete person.major
console.log(person)
console.log(Object.keys(person))
console.log(Object.values(person))
console.log(Object.entries(person))
var p2={}
Object.assign(p2,person,{major:"cs"})
console.log(p2)




for(var key in p2){
        console.log(key+" "+p2[key])
}
function Person(name,major){
        this.name=name
        this.major=major

}
var p3 = new Person("hamada","cyber")
var p4 = new Person("dodo","art")

function Animal(name){
        this.name =name
}
// console.log(Array.prototype)
Animal.prototype.sound=function(){
        console.log(this.name+"sound")
}
let a = new Animal("elephant")
a.sound()
function Cat(name){
        this.name=name
}
Cat.prototype=Object.create(Animal.prototype)
let c = new Cat("cat")
c.sound()
class Animall{
        constructor(name){
 this.name=name}
     sound(){
        console.log(this.name+"sound")
     }
}
var a2 = new Animall("DOG")
a2.sound()
class Catt extends Animall{
   constructor(name,breed)
   {super(name)
        this.breed=breed}
sound(){
console.log(this.name+"meow")
}
}
var a2 = new Animall("DOG")
a2.sound()
var c2 = new Catt("hello kitty","breed")
c2.sound()

let str = JSON.stringify(p2)
console.log(str)

let obj = JSON.parse(str)
console.log(obj)

da   php

<?php
// db.php - DB connection using PDO
$DB_HOST = '127.0.0.1';
$DB_NAME = 'web10';
$DB_USER = 'root';
$DB_PASS = ''; 

$dsn = "mysql:host=$DB_HOST;dbname=$DB_NAME;charset=utf8mb4";

$options = [
    PDO::ATTR_ERRMODE => PDO::ERRMODE_EXCEPTION,

    //[key => val]
];

try {
    $pdo = new PDO($dsn, $DB_USER, $DB_PASS,$options);
} catch (PDOException $e) {
    echo "<h1>Database connection failed: " . $e->getMessage() . "</h1>";
    exit;                       
}
